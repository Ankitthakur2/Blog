<!DOCTYPE html>
<html lang="en">
<head>
 
  <%- include('partials/head') %>

  <title>Blog</title>

</head>
<body>
  <%- include('partials/nav') %>


  <div class="container mt-3 vh-100 ">
  <%  blogs.forEach(blog => {  %>
  <div class="card mt-3 mx-3 d-inline-block" style="width: 18rem; ">
    <img src="<%= blog.coverImageURL %>" class="card-img-top" style="height: 200px;">
    <div class="card-body">
      <h5 class="card-title"><%= blog.title %></h5>

      <a href= "/blog/<%= blog._id %>" class="btn btn-primary" > View </a>
    </div>
  </div>
 <%  } ) %>
</div>
   <%- include('partials/scripts') %>

   <%- include('partials/footer') %>
</body>
</html>